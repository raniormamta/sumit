		import java.io.IOException;

import org.xml.sax.SAXException;
import org.xml.sax.XMLReader;
import org.xml.sax.helpers.XMLReaderFactory;


public class Driver {
	
		public static void main(String[] args) throws IOException,SAXException {
		XMLReader p = XMLReaderFactory.createXMLReader();
		p.setContentHandler(new ShHd());
		p.parse("D:\\SidhiVinayak\\3-12-2015\\Sample\\src\\ACS.xml");
		ShHd sh=new ShHd();
		}

}


public class Footer {
	private int fPId;
	private long recordCount;
	public Footer() {
		super();
		// TODO Auto-generated constructor stub
	}
	public Footer(int fPId, long recordCount) {
		super();
		this.fPId = fPId;
		this.recordCount = recordCount;
	}
	public int getfPId() {
		return fPId;
	}
	public void setfPId(int fPId) {
		this.fPId = fPId;
	}
	public long getRecordCount() {
		return recordCount;
	}
	public void setRecordCount(long recordCount) {
		this.recordCount = recordCount;
	}
	@Override
	public String toString() {
		return "Footer [fPId=" + fPId + ", recordCount=" + recordCount
				+ ", getRecordCount()=" + getRecordCount() + ", getfPId()="
				+ getfPId() + "]";
	}
}




public class Header {
